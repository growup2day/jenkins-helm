should include permanent nodes in default config script when configured:
  1: |
    |-
      jenkins:
        # Manage Nodes and Clouds > Manage Nodes
        nodes:
          - permanent:
              name: "nzdwXXXv"
              nodeDescription: "Permanent node nzdwXXXv.nz.unix.test"
              labelString: "nzdwXXXv vm maven chrome"
              launcher:
                ssh:
                  credentialsId: "jenkins-slave-cred"
                  host: "nzdwXXXv.nz.unix.test"
                  port: 22
                  sshHostKeyVerificationStrategy:
                    manuallyProvidedKeyVerificationStrategy:
                      key: "ssh-rsa host-public-key"
              mode: NORMAL
              nodeProperties:
              - toolLocation:
                  locations:
                  - home: /usr/lib/jvm/java-1.7.0-openjdk-1.7.0.75.x86_64
                    key: hudson.model.JDK$DescriptorImpl@JDK 1.7
                  - home: /usr/lib/jvm/java-1.8.0
                    key: hudson.model.JDK$DescriptorImpl@JDK 1.8
              numExecutors: 1
              remoteFS: /app/data/jenkins-slave
              retentionStrategy: always
