should include blank cloud in default config script when not configured:
  1: |
    |-
      jenkins:
        # Manage Nodes and Clouds > Configure Clouds
        clouds: []
should include openshift cloud in default config script when configured:
  1: |
    |-
      jenkins:
        # Manage Nodes and Clouds > Configure Clouds
        clouds:
          - kubernetes:
              name: cs1-dev
              serverUrl: "https://api.cs1-dev.nz.service.test:6443"
              namespace: "NAMESPACE"
              credentialsId: "cs1-dev-cred"
              webSocket: true
              jenkinsUrl: "https://RELEASE-NAME-jenkins-NAMESPACE.dummy.domain"
              defaultsProviderTemplate: "default-jenkins-slave"
              templates:
                - name: "default-jenkins-slave"
                  label: "openshift"
                  nodeSelector: "beta.kubernetes.io/os=linux"
                  serviceAccount: "RELEASE-NAME-jenkins"
                  idleMinutes: 30
                  instanceCap: 8
                  yamlMergeStrategy: override
                  containers:
                    - name: "jnlp"
                      command: "/usr/local/bin/run-jnlp-client"
                      args: "-webSocket ^${computer.jnlpmac} ^${computer.name}"
                      image: "image-registry.openshift-image-registry.svc:5000/openshift/jenkins-slave-base-ocp-anz:v4.7.2"
                      resourceLimitCpu: "2"
                      resourceLimitMemory: 4Gi
                      resourceRequestCpu: 200m
                      resourceRequestMemory: 500Mi
                      envVars:
                        - envVar:
                            key: "TZ"
                            value: "Pacific/Auckland"
                        - envVar:
                            key: "http_proxy"
                            value: ""
                        - envVar:
                            key: "https_proxy"
                            value: ""
                - name: "maven-jenkins-slave"
                  label: "openshift maven"
                  nodeSelector: "beta.kubernetes.io/os=linux"
                  serviceAccount: "RELEASE-NAME-jenkins"
                  idleMinutes: 30
                  instanceCap: 8
                  yamlMergeStrategy: override
                  containers:
                    - name: "jnlp"
                      command: "/usr/local/bin/run-jnlp-client"
                      args: "-webSocket ^${computer.jnlpmac} ^${computer.name}"
                      image: "image-registry.openshift-image-registry.svc:5000/openshift/jenkins-slave-maven-ocp-anz:v4.7.2"
                      resourceLimitCpu: "2"
                      resourceLimitMemory: 4Gi
                      resourceRequestCpu: 200m
                      resourceRequestMemory: 500Mi
                      envVars:
                        - envVar:
                            key: "TZ"
                            value: "Pacific/Auckland"
                        - envVar:
                            key: "http_proxy"
                            value: ""
                        - envVar:
                            key: "https_proxy"
                            value: ""
                - name: "nodejs-jenkins-slave"
                  label: "openshift nodejs"
                  nodeSelector: "beta.kubernetes.io/os=linux"
                  serviceAccount: "RELEASE-NAME-jenkins"
                  idleMinutes: 10
                  instanceCap: 8
                  yamlMergeStrategy: override
                  containers:
                    - name: "jnlp"
                      command: "/usr/bin/go-init"
                      args: "-main \"/usr/local/bin/run-jnlp-client -webSocket ^${computer.jnlpmac} ^${computer.name}\""
                      image: "image-registry.openshift-image-registry.svc:5000/openshift/jenkins-slave-nodejs-ocp-anz:v4.7.2"
                      resourceLimitCpu: "4"
                      resourceLimitMemory: 4Gi
                      resourceRequestCpu: 200m
                      resourceRequestMemory: 500Mi
                      workingDir: /tmp
                      envVars:
                        - envVar:
                            key: "CONCURRENT_BUILD_TASKS"
                            value: "2"
                        - envVar:
                            key: "TZ"
                            value: "Pacific/Auckland"
                        - envVar:
                            key: "http_proxy"
                            value: ""
                        - envVar:
                            key: "https_proxy"
                            value: "test.proxy"
          - kubernetes:
              name: cs2-dev
              serverUrl: "https://api.cs2-dev.nz.service.test:6443"
              namespace: "NAMESPACE"
              jenkinsTunnel: "RELEASE-NAME-jenkins-jnlp.NAMESPACE.svc.cluster.local:50000"
              jenkinsUrl: "https://RELEASE-NAME-jenkins-NAMESPACE.dummy.domain"
