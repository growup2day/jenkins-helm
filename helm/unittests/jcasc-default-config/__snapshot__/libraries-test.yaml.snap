should include blank pipeline libraries config in default config scripts when not configured:
  1: |
    |-
      unclassified:
        # Configure System > Global Pipeline Libraries
        globalLibraries:
          libraries: []
should include pipeline libraries config in default config scripts when configured:
  1: |
    |-
      unclassified:
        # Configure System > Global Pipeline Libraries
        globalLibraries:
          libraries:
            - name: "pipeline-lib1"
              defaultVersion: "master"
              includeInChangesets: false
              retriever:
                modernSCM:
                  scm:
                    git:
                      credentialsId: "anz-bitbucket-cred"
                      remote: "https://bitbucket.nz.service.anz/some/dummy-pipeline.git"
                      traits:
                        - "gitBranchDiscovery"
            - name: "pipeline-lib2"
              defaultVersion: "master"
              includeInChangesets: true
              retriever:
                modernSCM:
                  scm:
                    git:
                      remote: "https://bitbucket.nz.service.anz/some/other-dummy-pipeline.git"
                      traits:
                        - "gitBranchDiscovery"
